{
  "name": "readmeq",
  "version": "1.2.0",
  "description": "Just another one solution for modifying your README.md (not only) file",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "test": "jest",
    "dev": "npm run clean && tsc --watch",
    "build:docs": "node ./build/scripts/build-docs.js",
    "build:typedoc": "npx typedoc src/index.ts",
    "build:typedocmd": "npx typedoc --plugin typedoc-plugin-markdown src/index.ts",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kostixdev/readmeq.git"
  },
  "files": [
    "build/src/index.js"
  ],
  "keywords": [
    "readme",
    "keys",
    "modify"
  ],
  "author": "kostix.dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kostixdev/readmeq/issues"
  },
  "homepage": "https://github.com/kostixdev/readmeq#readme",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.3",
    "@types/jsdoc-to-markdown": "^7.0.3",
    "@types/node": "^14.11.2",
    "concat-md": "^0.5.1",
    "gts": "^3.1.1",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jsdoc-to-markdown": "^8.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.4",
    "typescript": "~4.7.0"
  }
}
